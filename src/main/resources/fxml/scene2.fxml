<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.VBox?>

<!-- FXML tiedosto Scene2 GUI-elementtien luomiseen. Tekijänä Jani Uotinen -->

<VBox fx:controller="GUI.ControllerForScene2"
      xmlns:fx="http://javafx.com/fxml" alignment="center">
    <children>
        <Label fx:id="label1" text="Uusi ikkuna!" alignment="center"/>
        <Button fx:id="button1" text="Nappi" onAction="#toiminnallisuus" alignment="center"/>
        <Label fx:id="errorLabel" text="" alignment="center"/>
        <Button fx:id="backtologin" text="Back to Login" onAction="#toLoginScreen" alignment="center"/>
        <Button fx:id="exit" text="Exit" onAction="#exit" alignment="center"/>
    </children>
</VBox>